{
  "extends": ["htmlvalidate:recommended"],
  "rules": {
    "no-inline-style": "warn",
    "require-sri": "off",
    "no-trailing-whitespace": "off",
    "void-style": "off",
    "attr-quotes": ["error", {"style": "double"}],
    "no-implicit-button-type": "error",
    "wcag/h37": "error",
    "wcag/h67": "error"
  },
  "elements": [
    "html5"
  ]
}
